<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="modello_di_dominio.Distinta" table="Distinta" lazy="false">
		<id name="ID" column="ID" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<property name="DataInizio" column="DataInizio" type="date" not-null="false" lazy="false"/>
		<many-to-one name="commessa" column="CommessaID" class="modello_di_dominio.Commessa" cascade="save-update,lock" unique="true" not-null="false" lazy="no-proxy" access="field">
		</many-to-one>
		<set name="ORM_RigheDistinta" lazy="true" cascade="all-delete-orphan" inverse="true">
			<key column="DistintaID" not-null="false"/>
			<one-to-many class="modello_di_dominio.RigaDistinta"/>
		</set>
	</class>
</hibernate-mapping>
